  // #FFF9F8 Coarse wool
  // #FFF9F8 Cold steel

  // #65618e Twilight purple
  // #B388E7 Lavender cream
  // #48AFC1 Tenzing
  // #48AFC180 Tenzing (subtle)
  // #31748f Adventure
  // #C2BEDD Chinese silver
  // #eb6f92 Colorada bronze
  // #E99C00 Just peachy
  // #BB9F68 Candy grass
  // #EA9A97 Angry ghost

  "tokenColors": [
    /* comment. */
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#C2BEDD",
        "fontStyle": "italic"
      }
    },
    /* constant. */
    {
      "scope": ["constant"],
      "settings": {
        "foreground": "#31748f"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.charcter.escape"
      ],
      "settings": {
        "foreground": "#EA9A97"
      }
    },
    /* entity. */
    {
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#EA9A97"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#48AFC1"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#E99C00"
      }
    },
    /* invalid. */
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ea7c66"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#BB9F68"
      }
    },
    /* keyword. */
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#31748f"
      }
    },
    /* markup. */
    /* meta. */
    {
      "scope": ["meta.tag", "meta.brace"],
      "settings": {
        "foreground": "#26233A"
      }
    },
    {
      "scope": ["meta.import", "meta.export"],
      "settings": {
        "foreground": "#31748f"
      }
    },
    /* punctuation. */
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#C2BEDD"
      }
    },
    {
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#31748f"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#E99C00"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#48AFC180"
      }
    },
    /* source. */
    /* storage. */
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#31748f"
      }
    },
    /* string. */
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#E99C00"
      }
    },
    /* support. */
    {
      "scope": ["support"],
      "settings": {
        "foreground": "#48AFC1"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#E99C00"
      }
    },
    {
      "scope": ["support.function"],
      "settings": {
        "foreground": "#eb6f92"
      }
    },
    /* text. */
    /* variable. */
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#EA9A97",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.other",
        "variable.language",
        "variable.function",
        "variable.argument"
      ],
      "settings": {
        "foreground": "#26233A"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#B388E7"
      }
    }
  ]
}
